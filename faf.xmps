<!-- 스택온도 48.5°C → 485 -->
<register address="30081" value="485"/>

<!-- AD0~AD3 -->
<register address="30082" value="12"/>
<register address="30083" value="38"/>
<register address="30084" value="50"/>
<register address="30085" value="33"/>

<!-- Restart time -->
<register address="30086" value="15"/>

<!-- 운전모드 RUN -->
<register address="30087" value="3"/>

<!-- Anti-PID 상태 -->
<register address="30088" value="1"/>

<!-- 누적 전력량 -->
<register address="30089" value="0"/>
<register address="30090" value="5120"/>
<register address="30091" value="35120"/>

<!-- 오늘 발전량 -->
<register address="30093" value="13200"/>

<!-- 피크 값 -->
<register address="30095" value="21000"/>
<register address="30097" value="16800"/>

<!-- Maximum Active Power -->
<register address="30099" value="2000"/>

</holding_registers>

<coils>
</coils>

<tuning>
<reply_delay min="0" max="0" />
