<!-- MAC -->
<register address="30024" value="202"/>
<register address="30025" value="168"/>
<register address="30026" value="15"/>

<!-- LOCAL IP: 192.168.0.50 -->
<register address="30027" value="192"/>
<register address="30028" value="168"/>
<register address="30029" value="0"/>
<register address="30030" value="50"/>

<!-- SUBNET MASK -->
<register address="30031" value="255"/>
<register address="30032" value="255"/>

<!-- REMOTE IP -->
<register address="30033" value="192"/>
<register address="30034" value="168"/>

<!-- PORT -->
<register address="30035" value="502"/>
<register address="30036" value="502"/>

<!-- BAUD RATE -->
<register address="30037" value="19200"/>
<register address="30038" value="0"/>
<register address="30039" value="8"/>
<register address="30040" value="0"/>

<!-- 현재 시각 2025-12-03 14:32:10 -->
<register address="30049" value="2025"/>
<register address="30050" value="12"/>
<register address="30051" value="3"/>
<register address="30052" value="14"/>
<register address="30053" value="32"/>
<register address="30054" value="10"/>

<!-- PV 입력 -->
<register address="30061" value="6800"/>   <!-- 680.0V -->
<register address="30062" value="125"/>    <!-- 12.5A -->
<register address="30063" value="85000"/>  <!-- 8.5kW -->

<!-- DCN Volt -->
<register address="30065" value="6400"/>

<!-- INV RS/ST/TR -->
<register address="30066" value="6900"/>
<register address="30067" value="6850"/>
<register address="30068" value="6880"/>

<!-- 계통 전압 -->
<register address="30069" value="2293"/>
<register address="30070" value="2301"/>
<register address="30071" value="2314"/>

<!-- 계통 전류 -->
<register address="30072" value="182"/>    <!-- 18.2 A -->
<register address="30073" value="178"/>
<register address="30074" value="180"/>

<!-- 유효전력 -->
<register address="30075" value="15500"/>

<!-- 무효전력 -->
<register address="30076" value="0"/>
<register address="30077" value="2200"/>

<!-- 역률 0.93 → scale 0.01 = 93 -->
