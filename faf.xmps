<?xml version="1.0"?>
<!DOCTYPE modbuspal_slave SYSTEM "modbuspal.dtd">
<modbuspal_slave>
<slave id="1" enabled="true" name="SLB_V200_REAL" implementation="modbus">
<holding_registers>

