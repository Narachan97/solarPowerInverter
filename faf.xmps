<register address="30009" value="50"/>   <!-- 2 -->
<register address="30010" value="49"/>   <!-- 1 -->
<register address="30011" value="48"/>   <!-- 0 -->
<register address="30012" value="56"/>   <!-- 8 -->
<register address="30013" value="50"/>   <!-- 2 -->
<register address="30014" value="52"/>   <!-- 4 -->
<register address="30015" value="65"/>   <!-- A -->
<register address="30016" value="48"/>   <!-- 0 -->

<!-- S/W Version -->
<register address="30017" value="10530"/>

<!-- 인버터 용량 -->
<register address="30018" value="2000"/>

<!-- STRING_NUM -->
<register address="30019" value="6"/>

